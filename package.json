{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prepare": "panda codegen",
    "dev": "NODE_ENV=development npm-run-all build --parallel dev:*",
    "dev:remix": "remix watch",
    "dev:panda": "panda codegen --watch",
    "dev:wrangler": "wrangler pages dev ./public",
    "build": "remix build",
    "build:prod": "NODE_ENV=production remix build",
    "start": "NODE_ENV=production wrangler pages dev ./public",
    "start2": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "typecheck": "tsc -p . --noEmit",
    "gen:migrate": "drizzle-kit generate:sqlite --out migrations --schema app/schema.ts",
    "release": "pnpm build:prod && wrangler pages publish ./public"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.18.1",
    "@remix-run/cloudflare-pages": "^1.18.1",
    "@remix-run/react": "^1.18.1",
    "cross-env": "^7.0.3",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230710.1",
    "@pandacss/dev": "^0.6.0",
    "@radix-ui/react-popover": "^1.0.6",
    "@remix-run/dev": "^1.18.1",
    "@remix-run/eslint-config": "^1.18.1",
    "@remix-run/node": "^1.18.1",
    "@remix-validated-form/with-zod": "^2.0.6",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "better-sqlite3": "^8.4.0",
    "drizzle-kit": "^0.19.5",
    "drizzle-orm": "^0.27.2",
    "eslint": "^8.44.0",
    "npm-run-all": "^4.1.5",
    "remix-auth": "^3.5.0",
    "remix-auth-form": "^1.3.0",
    "remix-auth-google": "^1.2.0",
    "remix-validated-form": "^5.0.2",
    "typescript": "^5.1.6",
    "wrangler": "^3.2.0",
    "zod": "^3.21.4"
  },
  "engines": {
    "node": ">=16.13"
  }
}
